<Page x:Class="MPLite.PageSetting"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MPLite"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="500"
      Title="PageSetting">

    <Grid Background="#FF323232">
        <StackPanel Height="50" VerticalAlignment="Top" Background="#FF2D2D2D">
            <Label x:Name="lbl_BasicSetting" Content="Basic" Height="24" Margin="0,13,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="#FFC8C8C8" Width="60" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Segoe UI" FontWeight="Bold"/>
        </StackPanel>
        <Grid Margin="0,50,0,0" Background="Black"/>
    </Grid>
</Page>
